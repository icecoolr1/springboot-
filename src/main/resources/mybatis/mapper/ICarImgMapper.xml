<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.jluzh.test_layuimini.mapper.ICarImgMapper">
    <insert id="addCarImg" parameterType="edu.jluzh.test_layuimini.bean.CarImg" useGeneratedKeys="true">
      INSERT INTO carimg (licenseNumber, imgPath) VALUES (#{licenseNumber},#{imgPath})
    </insert>
    <update id="updateImg" parameterType="edu.jluzh.test_layuimini.bean.CarImg"/>
    <delete id="deleteImg" parameterType="string">
        delete from carimg where licenseNumber = #{licenseNumber}
    </delete>
    <select id="findCarImg" resultType="edu.jluzh.test_layuimini.bean.CarImg">

    </select>
</mapper>